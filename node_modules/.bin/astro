#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@5.14.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_rollup@2.79.2_terser@5.44.0_typescript@5.8.3_yaml@2.8.1/node_modules/astro/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@5.14.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_rollup@2.79.2_terser@5.44.0_typescript@5.8.3_yaml@2.8.1/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@5.14.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_rollup@2.79.2_terser@5.44.0_typescript@5.8.3_yaml@2.8.1/node_modules/astro/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@5.14.7_@types+node@24.9.1_jiti@2.6.1_lightningcss@1.30.2_rollup@2.79.2_terser@5.44.0_typescript@5.8.3_yaml@2.8.1/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
