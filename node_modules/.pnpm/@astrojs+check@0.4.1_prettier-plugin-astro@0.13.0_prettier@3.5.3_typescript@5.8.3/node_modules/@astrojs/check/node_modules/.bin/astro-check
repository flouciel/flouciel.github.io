#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/@astrojs+check@0.4.1_prettier-plugin-astro@0.13.0_prettier@3.5.3_typescript@5.8.3/node_modules/@astrojs/check/dist/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/@astrojs+check@0.4.1_prettier-plugin-astro@0.13.0_prettier@3.5.3_typescript@5.8.3/node_modules/@astrojs/check/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/@astrojs+check@0.4.1_prettier-plugin-astro@0.13.0_prettier@3.5.3_typescript@5.8.3/node_modules/@astrojs/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/@astrojs+check@0.4.1_prettier-plugin-astro@0.13.0_prettier@3.5.3_typescript@5.8.3/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/@astrojs+check@0.4.1_prettier-plugin-astro@0.13.0_prettier@3.5.3_typescript@5.8.3/node_modules/@astrojs/check/dist/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/@astrojs+check@0.4.1_prettier-plugin-astro@0.13.0_prettier@3.5.3_typescript@5.8.3/node_modules/@astrojs/check/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/@astrojs+check@0.4.1_prettier-plugin-astro@0.13.0_prettier@3.5.3_typescript@5.8.3/node_modules/@astrojs/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/@astrojs+check@0.4.1_prettier-plugin-astro@0.13.0_prettier@3.5.3_typescript@5.8.3/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin.js" "$@"
else
  exec node  "$basedir/../../dist/bin.js" "$@"
fi
