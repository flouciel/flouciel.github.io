#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@4.16.2_@types+node@22.7.5_lightningcss@1.27.0_rollup@4.24.0_terser@5.34.1_typescript@5.6.3/node_modules/astro/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@4.16.2_@types+node@22.7.5_lightningcss@1.27.0_rollup@4.24.0_terser@5.34.1_typescript@5.6.3/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@4.16.2_@types+node@22.7.5_lightningcss@1.27.0_rollup@4.24.0_terser@5.34.1_typescript@5.6.3/node_modules/astro/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@4.16.2_@types+node@22.7.5_lightningcss@1.27.0_rollup@4.24.0_terser@5.34.1_typescript@5.6.3/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@4.16.2_@types+node@22.7.5_lightningcss@1.27.0_rollup@4.24.0_terser@5.34.1_typescript@5.6.3/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@4.16.2_@types+node@22.7.5_lightningcss@1.27.0_rollup@4.24.0_terser@5.34.1_typescript@5.6.3/node_modules/astro/astro.js" "$@"
fi
