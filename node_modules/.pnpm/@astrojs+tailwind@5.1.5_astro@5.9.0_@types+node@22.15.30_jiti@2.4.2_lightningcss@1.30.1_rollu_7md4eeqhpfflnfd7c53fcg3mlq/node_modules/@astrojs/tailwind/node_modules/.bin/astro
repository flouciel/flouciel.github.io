#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@5.9.0_@types+node@22.15.30_jiti@2.4.2_lightningcss@1.30.1_rollup@2.79.2_terser@5.41.0_typescript@5.8.3_yaml@2.8.0/node_modules/astro/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@5.9.0_@types+node@22.15.30_jiti@2.4.2_lightningcss@1.30.1_rollup@2.79.2_terser@5.41.0_typescript@5.8.3_yaml@2.8.0/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@5.9.0_@types+node@22.15.30_jiti@2.4.2_lightningcss@1.30.1_rollup@2.79.2_terser@5.41.0_typescript@5.8.3_yaml@2.8.0/node_modules/astro/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/astro@5.9.0_@types+node@22.15.30_jiti@2.4.2_lightningcss@1.30.1_rollup@2.79.2_terser@5.41.0_typescript@5.8.3_yaml@2.8.0/node_modules:/Users/4azy/lune/code/flouciel.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.9.0_@types+node@22.15.30_jiti@2.4.2_lightningcss@1.30.1_rollup@2.79.2_terser@5.41.0_typescript@5.8.3_yaml@2.8.0/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.9.0_@types+node@22.15.30_jiti@2.4.2_lightningcss@1.30.1_rollup@2.79.2_terser@5.41.0_typescript@5.8.3_yaml@2.8.0/node_modules/astro/astro.js" "$@"
fi
